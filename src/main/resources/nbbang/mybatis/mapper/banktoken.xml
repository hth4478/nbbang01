<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="nbbang.mybatis.mapper.bankaccount">

	<insert id="insertAuth" parameterType="Map">
		insert into banktoken values(#{access_token},#{email},#{user_seq_no},#{user_ci})
	</insert>
	
	<delete id="deleteToken" parameterType="Map">
		delete banktoken WHERE email = #{email}
	</delete>
	
	
	<select id="selectAuthMYOB" parameterType="Map" resultType="Map">
		SELECT * FROM banktoken WHERE email=#{email}
	</select>
	
	
	<select id="testInfo" parameterType="Map" resultType="Map">
		SELECT * FROM banktoken WHERE email=#{email}
	</select>
	
	
	
</mapper> 