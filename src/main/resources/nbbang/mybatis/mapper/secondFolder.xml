<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="nbbang.mybatis.mapper.secondFolder">
	
	<select id="secondFoldSelectList" parameterType="Map" resultType="secondFolderDTO" >
	<!-- 해당계정의 WATCH(두번째폴더)에 해당하는 레코드 읽어오기
		-필요파라미터(2) : email , sfno
		subno	subservice	payamentday	money	sfno
		32		유튜브프리미엄	22/01/29	10000	98
		33		핀즐			22/05/19	20000	98
		34		런드리고		22/01/08	65000	98 -->
		SELECT * FROM(SELECT s.* FROM subscription s 
		JOIN subfolder f ON f.sfno = s.sfno
		WHERE email=#{email})
		WHERE sfno=#{sfno}	
		<!--sfno: 해당유저 sfno긁어오는 서비스를 컨트롤러에서 주입받고 전처리(두번째폴더에 해당하는 파라미터넘기기)하기 -->
	</select>
	
</mapper> 